    "@react-navigation/stack": "7.3.4",
    "@reown/walletkit": "1.1.0",
    "@sentry/react-native": "6.10.0",
    "@walletconnect/core": "2.17.0",
    "@walletconnect/react-native-compat": "2.17.0",
    "assert": "2.0.0",
    "browserify-zlib": "0.1.4",
    "buffer": "4.9.2",
